//! base 0.1.0
package 'Enumeration'
  type 'State' = enumeration('off', 'starting', 'running', 'stopping') "Operating states";

  type 'Priority' = enumeration('low' "Low priority", 'medium' "Medium priority", 'high' "High priority");

  model 'Enumeration' "Model using enumeration types and literals"
    Real 't'(start = 0.0);
    Real 'offValue';
    Real 'runningValue';
    Real 'lowValue';
    Real 'highValue';
  equation
    der('t') = 1.0;
    // Test enum literals directly - they convert to integers (0-based)
    // State: off=0, starting=1, running=2, stopping=3
    // Priority: low=0, medium=1, high=2
    'offValue' = 'State'.'off';
    'runningValue' = 'State'.'running';
    'lowValue' = 'Priority'.'low';
    'highValue' = 'Priority'.'high';
  end 'Enumeration';
end 'Enumeration';

