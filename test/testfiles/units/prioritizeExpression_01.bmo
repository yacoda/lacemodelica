//! base 0.1.0
package 'prioritizeExpression_01'
  model 'M'
    Real 'x';
    parameter equation guess('x') = prioritize(5.0, 1);
  equation
    'x' * 'x' = 25.0;
  end 'M';
end 'prioritizeExpression_01';

