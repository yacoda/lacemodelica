//! base 0.1.0
package 'STRING_01'
  model 'M' "A model with string comments"
    Real 'x' "hello";
    Real 'y' "line1\nline2";
    Real 'z' "";
  equation
    'x' = 1.0 "first eq";
    'y' = 2.0;
    'z' = 3.0;
  end 'M';
end 'STRING_01';

