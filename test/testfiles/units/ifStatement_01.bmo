//! base 0.1.0
package 'ifStatement_01'
  function 'abs_val'
    input Real 'u';
    output Real 'y';
  algorithm
    if 'u' < 0.0 then
      'y' := -'u';
    else
      'y' := 'u';
    end if;
  end 'abs_val';
  model 'M'
    Real 'x';
  equation
    'x' = 'abs_val'(-3.0);
  end 'M';
end 'ifStatement_01';

