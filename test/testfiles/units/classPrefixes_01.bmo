//! base 0.1.0
package 'classPrefixes_01'
  pure function 'twice'
    input Real 'u';
    output Real 'y';
  algorithm
    'y' := 'u' * 2.0;
  end 'twice';
  model 'M'
    Real 'x';
  equation
    'x' = 'twice'(1.5);
  end 'M';
end 'classPrefixes_01';

