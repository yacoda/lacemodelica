//! base 0.1.0
package 'namedArgument_01'
  function 'add'
    input Real 'a';
    input Real 'b';
    output Real 'c';
  algorithm
    'c' := 'a' + 'b';
  end 'add';
  model 'M'
    Real 'x';
  equation
    'x' = 'add'(a = 1.0, b = 2.0);
  end 'M';
end 'namedArgument_01';

