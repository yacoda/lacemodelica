//! base 0.1.0
package 'ifExpression_02'
  model 'M'
    Real 'x';
    Real 'y';
  equation
    'x' = -3.0;
    'y' = if 'x' > 0.0 then 1.0 elseif 'x' < 0.0 then -1.0 else 0.0;
  end 'M';
end 'ifExpression_02';

