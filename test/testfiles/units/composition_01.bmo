//! base 0.1.0
package 'composition_01'
  model 'M'
    Real 'x';
    Real 'y';
  initial equation
    'y' = 0.0;
  equation
    'x' = 1.0;
    der('y') = 'x';
  end 'M';
end 'composition_01';

