//! base 0.1.0
package 'simpleExpression_03'
  function 'sum_step'
    output Real 'y';
  algorithm
    'y' := 0.0;
    for 'i' in 1:2:5 loop
      'y' := 'y' + 'i';
    end for;
  end 'sum_step';
  model 'M'
    Real 'x';
  equation
    'x' = 'sum_step'();
  end 'M';
end 'simpleExpression_03';

