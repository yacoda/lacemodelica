//! base 0.1.0
package 'subscript_01'
  model 'M'
    Real 'arr'[3];
    Real 'x';
  equation
    'arr'[1] = 1.0;
    'arr'[2] = 2.0;
    'arr'[3] = 3.0;
    'x' = 'arr'[1 + 1];
  end 'M';
end 'subscript_01';

