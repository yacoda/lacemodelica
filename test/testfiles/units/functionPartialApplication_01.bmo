//! base 0.1.0
package 'functionPartialApplication_01'
  function 'add'
    input Real 'a';
    input Real 'b';
    output Real 'y';
  algorithm
    'y' := 'a' + 'b';
  end 'add';
  model 'M'
    Real 'x';
  equation
    // Test partial application syntax: function name(named_args)
    'x' = 'add'(function 'add'(a = 10.0, b = 5.0));
  end 'M';
end 'functionPartialApplication_01';

