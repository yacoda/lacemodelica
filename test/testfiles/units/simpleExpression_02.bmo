//! base 0.1.0
package 'simpleExpression_02'
  function 'sum_range'
    input Integer 'n';
    output Real 'y';
  algorithm
    'y' := 0.0;
    for 'i' in 1:'n' loop
      'y' := 'y' + 'i';
    end for;
  end 'sum_range';
  model 'M'
    Real 'x';
  equation
    'x' = 'sum_range'(5);
  end 'M';
end 'simpleExpression_02';

