//! base 0.1.0
package 'logicalFactor_01'
  model 'M'
    Real 'x';
    Real 'y';
  equation
    'x' = 5.0;
    'y' = if 'x' > 0.0 then 1.0 else 0.0;
  end 'M';
end 'logicalFactor_01';

