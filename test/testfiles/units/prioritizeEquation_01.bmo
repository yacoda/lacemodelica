//! base 0.1.0
package 'prioritizeEquation_01'
  model 'M'
    Real 'x';
    Real 'y';
  initial equation
    prioritize('x', 1);
    prioritize('y', 2);
  equation
    der('x') = 1.0;
    der('y') = 'x';
  end 'M';
end 'prioritizeEquation_01';

