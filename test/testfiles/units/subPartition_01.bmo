//! base 0.1.0
package 'subPartition_01'
  model 'M'
    Real 'x';
  equation
    'x' = 1.0;
  partition "main"
    Clock 'clk' = 0.1;
    subpartition(solver = ImplicitEuler)
    equation
      'x' = 2.0;
  end 'M';
end 'subPartition_01';

