//! base 0.1.0
package 'relation_01'
  model 'M'
    Real 'a';
    Real 'b';
    Real 'x';
  equation
    'a' = 3.0;
    'b' = 5.0;
    'x' = if 'a' > 'b' then 1.0 else 0.0;
  end 'M';
end 'relation_01';

