//! base 0.1.0
package 'outputExpressionList_01'
  function 'swap'
    input Real 'a';
    input Real 'b';
    output Real 'x';
    output Real 'y';
  algorithm
    'x' := 'b';
    'y' := 'a';
  end 'swap';
  model 'M'
    Real 'p';
    Real 'q';
  equation
    ('p', 'q') = 'swap'(1.0, 2.0);
  end 'M';
end 'outputExpressionList_01';

