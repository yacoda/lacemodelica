//! base 0.1.0
package 'namedArguments_01'
  function 'f'
    input Real 'a';
    input Real 'b';
    output Real 'y';
  algorithm
    'y' := 'a' - 'b';
  end 'f';
  model 'M'
    Real 'x';
  equation
    'x' = 'f'(b = 1.0, a = 5.0);
  end 'M';
end 'namedArguments_01';

