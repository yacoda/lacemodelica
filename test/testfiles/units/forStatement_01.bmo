//! base 0.1.0
package 'forStatement_01'
  function 'sum3'
    output Real 'y';
  algorithm
    'y' := 0.0;
    for 'i' in 1:3 loop
      'y' := 'y' + 'i';
    end for;
  end 'sum3';
  model 'M'
    Real 'x';
  equation
    'x' = 'sum3'();
  end 'M';
end 'forStatement_01';

