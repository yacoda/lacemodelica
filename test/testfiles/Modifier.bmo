//! base 0.1.0
package 'Modifier'
  model 'Modifier'
    Real 'x'(fixed = true, start = 1.0);
    parameter Real 'a'(nominal = 100.0, start = 400.0) = 10.0;
  equation
    der('x') = 'a'*'x';
  end 'Modifier';
end 'Modifier';

